<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:busterace = "http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.busterace.myviewdemo.MainActivity">

    <com.busterace.viewlibrary.CommonTitleView
        android:id="@+id/common_title_view"
        android:background="@android:color/holo_blue_light"
        android:layout_width="match_parent"
        busterace:middle_text = "dddddd"
        busterace:left_text = "aaaa"
        busterace:left_drawable="@android:drawable/ic_delete"
        busterace:right_text = "cccc"
        busterace:right_drawable="@android:drawable/ic_delete"
        android:layout_height="40dp" />

</LinearLayout>
